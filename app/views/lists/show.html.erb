<h2 class="title"><%= @list.title %></h2>
<p class="list_sub_title"><%= @list.description %></p>


<div id="items_wrapper">
	<%= render @list.items %>
	<div id="form">
		<%= render "items/form" %>
	</div>
</div>

<div class="links">
	<%= link_to "Back", :root %>
	<%= link_to "Delete", list_path(@list), method: :delete, data: { confirm: "Are you sure?" } %>
	<%= link_to "Edit", edit_list_path(@list) %>
</div>